    <supports>
      <mime-type>text/html</mime-type>
    </supports>
    <supported-locale>en</supported-locale>
    <portlet-info>
      <title>TestCache</title>
      <short-title>TestCache</short-title>
      <keywords>TestCache</keywords>
    </portlet-info>
  </portlet>

