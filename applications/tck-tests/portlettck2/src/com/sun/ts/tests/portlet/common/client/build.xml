<project name="servlet_common_util" basedir="." default="usage" >
	<property name="pkg.name"   value="client" />
	<property name="pkg.dir"   value="com/sun/ts/tests/portlet/common/{pkg.name}" />

	<target name="build" depends="compile" />


	<target name="clean" >
		<ant antfile="build.xml" dir="${ts.home}/bin" target="clean" />
	</target>
	
	<target name="compile" >
		<ant antfile="build.xml" dir="${ts.home}/bin" target="compile" />
	</target>
	
	<target name="deploy"/>

	<target name="package"/>

	<target name="runclient"/>

	<target name="usage" >
		<ant antfile="build.xml" dir="${ts.home}/bin" target="usage" />
	</target>

</project>
