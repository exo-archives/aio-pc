<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
          "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

  <form-beans>
  </form-beans>

  <global-exceptions>
    <exception key="errors.general" type="java.lang.Exception" path="/error.jsp"/>
  </global-exceptions>

  <global-forwards>
    <forward name="failure" path="/error.do"/>
    <forward name="error" path="/error.do"/>
  </global-forwards>

  <action-mappings>
    <action path="/view" include="/view.jsp"/>
    <action path="/edit" include="/edit.jsp"/>
    <action path="/gov" include="/gov.jsp"/>
    <action path="/goe" include="/goe.jsp"/>
    <action path="/error" include="/error.jsp"/>

  </action-mappings>

  <controller pagePattern="$M$P" inputForward="false" processorClass="org.apache.portals.bridges.struts.PortletRequestProcessor"/>

</struts-config>
